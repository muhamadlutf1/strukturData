package com.mycompany.tugas_strukturdata2;

import java.util.HashMap;
import java.util.Map;
import java.util.Set;
import java.util.HashSet;
public class Tugas_strukturData2 {

    public static void main(String[] args) {
        int[] numbers = {1, 2, 2, 3, 3, 4, 5, 5};
        
        // Membuat HashMap untuk menghitung frekuensi setiap angka
        Map<Integer, Integer> frequencyMap = new HashMap<>();
        
        // Menghitung frekuensi setiap angka
        for (int number : numbers) {
            frequencyMap.put(number, frequencyMap.getOrDefault(number, 0) + 1);
        }
        
        // Menyiapkan Set untuk menyimpan elemen unik
        Set<Integer> uniqueElements = new HashSet<>();
        
        // Memeriksa angka dengan frekuensi 1
        for (Map.Entry<Integer, Integer> entry : frequencyMap.entrySet()) {
            if (entry.getValue() == 1) {
                uniqueElements.add(entry.getKey());
            }
        }
        
        // Menampilkan hasil sesuai kondisi
        if (!uniqueElements.isEmpty()) {
            System.out.println("ada = " + uniqueElements);
        } else {
            System.out.println("ga ada = [ ]");
        }
    }
}
